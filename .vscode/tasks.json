{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build: Debug",
			"command": "${command:chalet-tools.makeDebugBuild}",
			"type": "process",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOn":[
				"MSBuild: Debug"
			],
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "MSBuild: Debug",
			"command": "./msbuild.sh",
			"type": "shell",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}